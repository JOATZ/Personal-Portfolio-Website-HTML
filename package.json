{
  "name": "RyanZiermanProfilePage",
  "version": "1.0.0",
  "description": "Portfolio project and Future Profile Website",
  "main": "index.html",
  "scripts": {
    "babel": "babel js/* -d js",
    "build": "npm run clean && npm run copyjs && npm run babel && npm run imagemin && npm run copyfonts && npm run usemin",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "copyjs": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.bundle.js js",
    "imagemin": "imagemin img/* -o dist/img",
    "lite": "lite-server",
    "scss": "sass css:scss",
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "usemin": "usemin inner-page.html -d dist --htmlmin -o dist/inner-page.html && usemin porfolio-details.html -d dist --htmlmin -o dist/potfolio-details.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "auhtor": "Ryan Zierman w/@iPortfolio-PRO license",
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.6.1",
    "node-sass": "^8.0.0",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "5.3",
    "bootstrap-social": "^5.1.1",
    "boxicons": "^2.1.4",
    "font-awesome": "x.x.x",
    "npm": "^9.7.2"
  }
}
